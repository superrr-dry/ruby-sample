<h1>Tests</h1>
<div>
  <% @tests.each do |test| %>
    <div style="display: flex;">
        <div style="width: 200px;">Content: <%= test.content %></div>
        <div><%= button_to "edit", edit_test_path(test), method: :get %></div>
        <div><%= button_to "delete", test_path(test), method: :delete %></div>
    </div>
  <% end %>
</div>
<%= link_to "New test", new_test_path %>